update lineorder
set lo_tax = lo_tax
where lo_orderkey = 1000000;

update lineorder_query_low
set lo_tax = lo_tax
where lo_orderkey = 1000000;

update lineorder_query_high
set lo_tax = lo_tax
where lo_orderkey = 1000000;

update lineorder_archive_low
set lo_tax = lo_tax
where lo_orderkey = 1000000;

update lineorder_archive_high
set lo_tax = lo_tax
where lo_orderkey = 1000000;
